{
  "struct": {
    "prefix": ["st"],
    "body": [
      "struct $1 {",
      "\t$0",
      "};"
    ]
  },
  "class": {
    "prefix": ["cls"],
    "body": [
      "class $1 {",
      "public:",
      "\t$2",
      "private:",
      "\t$0",
      "};"
    ]
  },
  "fn": {
    "prefix": ["fn"],
    "body": [
      "$1 $2($3) {",
      "\t${0:pass}",
      "}"
    ]
  },
  "fn/auto": {
    "prefix": ["afn"],
    "body": [
      "auto $1($2) -> $3 {",
      "\t$0",
      "}"
    ]
  },
  "if": {
    "prefix": ["if"],
    "body": [
      "if ($1) {",
      "\t$0",
      "}"
    ]
  },
  "if/guard": {
    "prefix": ["gif"],
    "body": [
      "if ($1) return$0;"
    ]
  },
  "for": {
    "prefix": ["for"],
    "body": [
      "for ($1; $2; $3) {",
      "\t$0",
      "}"
    ]
  },
  "for/range": {
    "prefix": ["forr"],
    "body": [
      "for ($1 : $2) {",
      "\t$0",
      "}"
    ]
  },
  "switch": {
    "prefix": ["switch"],
    "body": [
      "switch ($1) {",
      "\t$0",
      "}"
    ]
  }
}
