{
  "function": {
    "prefix": ["fun"],
    "body": [
      "function! $1($2) abort",
      "\t$0",
      "endfunction"
    ]
  },
  "if": {
    "prefix": ["if"],
    "body": [
      "if $1",
      "\t$0",
      "endif"
    ]
  },
  "aug": {
    "prefix": ["aug"],
    "body": [
      "augroup $1",
      "\tautocmd!",
      "\tautocmd $0",
      "augroup END"
    ]
  }
}
