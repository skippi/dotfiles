{
  "function": {
    "prefix": ["fu"],
    "body": [
      "function! $1($2) abort",
      "\t$0",
      "endfunction"
    ]
  },
  "if": {
    "prefix": ["if"],
    "body": [
      "if $1",
      "\t$0",
      "endif"
    ]
  },
  "augroup": {
    "prefix": ["ag"],
    "body": [
      "augroup $1",
      "\tautocmd!",
      "\tautocmd $0",
      "augroup END"
    ]
  },
  "try": {
    "prefix": ["tr"],
    "body": [
      "try $1",
      "\t$0",
      "endtry"
    ]
  }
}
